<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>
  <table name="btTdsHighlightFile">
    <field name="bID" type="integer">
      <unsigned/>
      <key/>
    </field>
    <field name="highlightFileID" type="integer">
      <unsigned/>
    </field>
    <field name="highlightFileName" type="text" size="128" />
    <field name="noLines" type="integer" size="8" />
    <field name="winHeight" type="integer" size="8" />
    <field name="aceType" type="text" size="128" />
  </table>
</schema>